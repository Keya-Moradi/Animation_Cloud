    <h1 class="h3 mb-3 fw-normal">Edit Profile <%= account.accountNumber %></h1>

    <div class="form-floating">
      <input type="text" class="col-sm-9 text-secondary" id="accountName" name="accountName" placeholder="Account Name">
      <label for="accountName"><%= email %></label>
    </div>c

    <button class="mb-0" type="submit">Update Profile</button>
    <a href="/profile/edit<%= account.accountNumber %>" class="btn btn-danger">Cancel Update</a>
    <p class="mt-5 mb-3 text-muted">&copy; 2023</p>
  </form>
</main>

